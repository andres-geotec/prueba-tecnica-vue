{
  "name": "prueba-tecnica-vue",
  "version": "1.0.0",
  "description": "Prueba técnica desarrollada por [Andrés Martínez González](https://github.com/andres-geotec) en Vue 3.",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "format-lint": "npm run format && npm run lint",
    "reinstall": "rm -rf node_modules && rm package-lock.json && npm cache clean -f && npm cache verify && npm i"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitest/coverage-v8": "^1.6.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/test-utils": "^2.4.5",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.4.0",
    "eslint-plugin-vue": "^9.23.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.5",
    "postcss": "^8.4.38",
    "prettier": "^3.3.1",
    "tailwindcss": "^3.4.4",
    "vite": "^5.2.13",
    "vitest": "^1.4.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "https://codigo.conahcyt.mx/sisdai/sisdai-mapas"
  },
  "bugs": {
    "url": "https://codigo.conahcyt.mx/sisdai/sisdai-mapas/issues"
  },
  "homepage": "https://codigo.conahcyt.mx/sisdai/sisdai-mapas#readme"
}
